{
  "name": "resume",
  "version": "0.1.0",
  "description": "Jean Farrugia's Resume",
  "main": "resume.json",
  "dependencies": {
    "cheerio": "^1.0.0-rc.6",
    "jsonresume-theme-stackoverflow-FA5": "github:frg/jsonresume-theme-stackoverflow#feature/string-location-support",
    "resume-cli": "^3.0.4"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "del-cli": "^3.0.1",
    "rename-cli": "^6.2.1"
  },
  "scripts": {
    "generate": "resume export ./dist/resume.html --format html --theme jsonresume-theme-stackoverflow-FA5 && npm run ghpages && node gainject.js",
    "ghpages": "copyfiles -f \"./dist/resume.html\" ./ && npm run clean-ghpages && rename resume.html index.html",
    "clean-ghpages": "del -f index.html || del -f index.html",
    "test": "resume validate resume.json"
  },
  "keywords": [
    "resume",
    "jean",
    "farrugia",
    "jean",
    "auto"
  ],
  "author": "Jean Farrugia",
  "license": "MIT"
}
